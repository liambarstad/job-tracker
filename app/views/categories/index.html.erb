<h1>All Categories</h1>

<ul>
  <% @categories.each do |category| %>
    <li>
      <a><%= link_to category.title, category_path(category) %></a>
      <a><%= link_to "Edit", edit_category_path(category) %></a>
      <a><%= link_to "Delete", category_path(category), method: :delete %></a>
    </li>
  <% end %>
</ul>
